{{#each this.hits}}
    <div class="gallery">
<a href="{{largeImageURL}}">
    <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" /></a>

    <div class="info">
    <table class="table-v " style = "width: 100%;" border="0" >
        <tr>
            <th >Likes–∞</th>
            <th>Views</th>
            <th>Comments</th>
            <th>Downloads</th>
        </tr>
        <tr>
            <td style = "width: 100px;">{{likes}}</td>
            <td>{{views}}</td>
            <td>{{comments}}</td>
            <td>{{downloads}}</td>
        </tr>
        
    </table>

    </div>
</div>
{{/each}}


